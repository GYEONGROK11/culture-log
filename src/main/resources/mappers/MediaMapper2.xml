<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.movieflow.media.MediaMapper2">
    <select id="selMedia">
        SELECT imedia FROM t_media
        WHERE iuser = #{iuser}
        AND imedia = #{imedia}
    </select>
    <delete id="delMediaPics">
        DELETE FROM t_media
        WHERE imedia = #{imedia}
    </delete>
    <delete id="delMedia">
        DELETE FROM t_media
        WHERE imedia = #{imedia}
        AND iuser = #{iuser}
    </delete>
</mapper>
